{
	"version": "0.1.0",
	"command": "swiftc",
	"isShellCommand": true,
	"showOutput": "silent",
	"args": [ "-Onone", "-g", "main.swift", "-o", "out/main"],
	"owner": "swift",
	"pattern": {
		"regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
		"file": 1,
		"line": 2,
		"column": 3,
		"severity": 4,
		"message": 5
	}
}
